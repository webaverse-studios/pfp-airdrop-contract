  "dependencies": {
    "@a16z/contracts": "^0.0.8",
    "@openzeppelin/contracts": "^4.7.0",
    "@openzeppelin/contracts-upgradeable": "^4.7.0",
    "@openzeppelin/truffle-upgrades": "^1.15.0",
    "@truffle/hdwallet-provider": "^2.0.11",
    "dotenv": "^16.0.1",
    "erc721a": "^4.2.3",
    "openzeppelin-contracts": "^4.0.0",
