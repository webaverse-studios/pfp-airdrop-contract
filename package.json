{
  "name": "contracts",
  "version": "1.0.0",
  "description": "WebaverseCharacterCreator Contracts",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webaverse-studios/WebaverseCharacterCreator-Contract.git"
  },
  "scripts": {
    "deploy-mainnet": "truffle migrate --network mainnet",
    "deploy-mainnet-reset": "truffle migrate --network mainnet --reset",
    "deploy-goerli": "truffle migrate --network goerli",
    "deploy-goerli-reset": "truffle migrate --network goerli --reset",
    "deploy-polygon": "truffle migrate --network polygon",
    "deploy-polygon-reset": "truffle migrate --network polygon --reset",
    "deploy-verify-mainnet": "truffle run verify WebaverseCharacter --network mainnet",
    "deploy-verify-polygon": "truffle run verify WebaverseCharacter --network polygon",
    "deploy-verify-goerli": "truffle run verify WebaverseCharacter --network goerli"
  },
